{
  "name": "mortarjs",
  "version": "1.0.0",
  "homepage": "http://mortar.fuzzpro.com",
  "description": "Mortar JS - an Admin UI framework",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build-js && npm run build-css",
    "build-js": "babel lib --out-dir dist",
    "build-css": "node-sass lib/styles/main.scss dist/styles/main.css",
    "test": "mocha --compilers .:tests/helpers/compiler.js tests/*.js",
    "document": "jsdoc -p -r index.js lib -d documentation/ -R README.md -t ./node_modules/ink-docstrap/template -c jsdoc.conf.json && open documentation/index.html",
    "doc": "jsdoc -p -r index.js lib -d documentation/ -R README.md -t ./node_modules/ink-docstrap/template -c jsdoc.conf.json"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Fuzz Productions <web@fuzzproductions.com> (http://www.fuzzproductions.com)",
  "contributors": [
    "Simon Yousoufov <syousoufov@fuzzproductions.com>",
    "Kyle Mendes <kmendes@fuzzproductions.com>"
  ],
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "classnames": "^2.1.1",
    "flux": "^2.0.3",
    "fuse.js": "^1.3.0",
    "ink-docstrap": "^1.1.0",
    "js-schema": "git+https://github.com/fuzz-productions/js-schema.git",
    "lodash": "^3.9.3",
    "medium-editor": "^5.6.0",
    "react": "^0.13.x",
    "react-bootstrap": "^0.22.6",
    "react-input-calendar": "^0.1.19",
    "react-router": "^0.13.3",
    "react-time-picker": "^1.1.0",
    "reqwest": "^1.1.5",
    "validator": "^3.40.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-minify-css": "^1.2.3",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-uglify": "^1.5.2",
    "jsdoc": "^3.4.0",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "node-sass": "^3.4.2",
    "react-tools": "^0.13.3"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "env": {
      "es6": true
    }
  }
}

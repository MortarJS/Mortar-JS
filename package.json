    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-uglify": "^1.5.2",
    "ink-docstrap": "^1.1.0",
    "jsdoc": "^3.4.0",
    "react-tools": "^0.13.3"

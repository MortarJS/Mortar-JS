  ],
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.1.1",
    "flux": "^2.0.3",
    "fuse.js": "^1.3.0",
    "js-schema": "git+https://github.com/fuzz-productions/js-schema.git",
    "lodash": "^3.9.3",
    "medium-editor": "^5.6.0",
    "react": "^0.13.x",
